<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html>
<head>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js" type="text/javascript"></script>
<script src="http://code.jquery.com/ui/1.9.1/jquery-ui.js"></script>

<script>

window.onload = function(){
	
	$('.clicktohide').siblings().hide();
	
	$('body').on('click', '.clicktohide',function(){
    	$(this).siblings().slideToggle('fast');
	});

	
	$('.draggableCat').draggable({
		connectToSortable: ".sortableCats",
		helper: "clone"
	});
	
	$('.draggableItem').draggable({
		connectToSortable: ".sortableItems",
		helper: "clone"
	});
	
	$('.sortableItems, .sortableCats').sortable();
	
	$("ul, li").disableSelection();
	
}
var list = [];
function readJSONObject(key, val){
	if(typeof val === 'object'){
		$.each(val, function(k, v){
			readJSONObject(k,v);
		});
	}
	else{
		list.push('<li>' + key + ': ' + val + '</li>');
	}
}

$.getJSON("pdftest/cv.json", function(data){
	$.each(data, function(key, val){
		readJSONObject(key, val);
	});
	
	$('<ul/>', {
    	'class': 'list',
    	html: list.join('')
  	}).appendTo('body');
});

</script>
</head>
<body>
<script>

</script>
</body>
</html>